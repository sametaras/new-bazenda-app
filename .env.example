# Environment Configuration Example
# Bu dosyayı .env olarak kopyalayın ve kendi değerlerinizle doldurun

# Environment (development, staging, production)
EXPO_PUBLIC_ENVIRONMENT=development

# API Configuration
EXPO_PUBLIC_API_URL=https://bazenda.com/api

# Analytics (production için)
EXPO_PUBLIC_ENABLE_ANALYTICS=false

# Error Reporting (production için)
EXPO_PUBLIC_ENABLE_ERROR_REPORTING=false

# Debug Mode
EXPO_PUBLIC_DEBUG_MODE=true

# Feature Flags (isteğe bağlı)
EXPO_PUBLIC_ENABLE_PUSH_NOTIFICATIONS=false
EXPO_PUBLIC_ENABLE_DARK_MODE=false
